      <Discovery ID="PureStorage.FlashArrays.Discovery.ArrayQueues" Target="PureStorage.FlashArrays.ComputerRole.CentralProcessor" Enabled="true" ConfirmDelivery="false" Remotable="true" Priority="Normal">
        <Category>Discovery</Category>
