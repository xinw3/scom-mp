      <Discovery ID="PureStorage.FlashArrays.Pure.FlashArrays.Discovery.Arrays" Target="Windows!Microsoft.Windows.Server.OperatingSystem" Enabled="true" ConfirmDelivery="false" Remotable="true" Priority="Normal">
